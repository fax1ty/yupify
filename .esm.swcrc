{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "jsc": {
    "transform": null,
    "keepClassNames": true,
    "parser": {
      "syntax": "typescript",
      "dynamicImport": true
    },
    "target": "es5",
    "loose": true
  },
  "module": {
    "noInterop": false,
    "type": "es6",
    "importInterop": "node"
  },
  "minify": true
}
